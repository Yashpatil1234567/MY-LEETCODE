bool isAnagram(char* s, char* t) {
    int i ;
    if(strlen(s)!=strlen(t))
    {
        return(false);
    }
    else
    {
        int counts[256]={0};
        for(i=0;s[i]!='\0';i++)
        {
            counts[s[i]]++;
            counts[t[i]]--;
        }
        for(i=0;i<256;i++)
        {
            if(counts[i]!=0)
            {
                return(false);
            }
        }return(true);
    }
    
}
